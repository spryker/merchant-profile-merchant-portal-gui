{
  "name": "spryker/merchant-profile-merchant-portal-gui",
  "type": "library",
  "description": "MerchantProfileMerchantPortalGui module",
  "license": "proprietary",
  "require": {
    "php": ">=7.3",
    "spryker/country": "^3.0.0",
    "spryker/glossary": "^3.0.0",
    "spryker/kernel": "^3.46.0",
    "spryker/locale": "^3.0.0",
    "spryker/merchant": "^3.0.0",
    "spryker/merchant-profile": "^0.3.0",
    "spryker/merchant-user": "^0.5.0 || ^0.6.0",
    "spryker/symfony": "^3.0.0",
    "spryker/url": "^3.0.0",
    "spryker/zed-ui": "^0.3.2"
  },
  "require-dev": {
    "spryker/code-sniffer": "*"
  },
  "autoload": {
    "psr-4": {
      "Spryker\\": "src/Spryker/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {
    "cs-check": "phpcs -p -s --standard=vendor/spryker/code-sniffer/Spryker/ruleset.xml src/",
    "cs-fix": "phpcbf -p --standard=vendor/spryker/code-sniffer/Spryker/ruleset.xml src/"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  },
  "config": {
    "sort-packages": true
  }
}
